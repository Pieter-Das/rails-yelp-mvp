<h1>New Restaurant</h1>

<%= form_with(model: @restaurant) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>
  <div>
    <%= form.label :phone_number %>
    <%= form.text_field :phone_number %>
  </div>
  <div>
    <%= form.label :category %>
    <%= form.text_field :category %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
